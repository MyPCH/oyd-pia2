  "paths": {
    "/oauth/token": {
      "post": {
        "summary": "request token",
        "tags": [
          "Authorization"
        ],
        "consumes": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "input",
            "in": "body"
          }
        ],
        "responses": {
          "200": {
            "description": "success"
          },
          "401": {
            "description": "invalid"
          }
        }
      }
    }
  },
  "securityDefinitions": {
    "Bearer": {
      "description": "Authorization token",
      "type": "apiKey",
      "name": "Authorization",
      "in": "header"
    }
  }
}